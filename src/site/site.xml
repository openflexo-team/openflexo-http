<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
   
	<bannerLeft>
		<name>Technology Adapters</name>
		<src>${ta.logo}</src>
		<href>https://openflexo-ta.openflexo.org</href>
	</bannerLeft>
	<bannerRight>
		<name>Openflexo</name>
		<src>${organization.logo}</src>
		<href>http://openflexo.org/</href>
	</bannerRight>

	<skin>
		<groupId>org.openflexo</groupId>
		<artifactId>openflexo-skin</artifactId>
		<version>1.3-SNAPSHOT</version>
	</skin>

	<custom>
		<openflexo-skin>
			<sideMenus>true</sideMenus>
			<displayBreadCrumbs>true</displayBreadCrumbs>
			<localMenu>/inc/html/developersMenu.html</localMenu>
			<includeVersionLinks>true</includeVersionLinks>
		</openflexo-skin>
	</custom>

	<body>

    <menu name=""
			img="https://support.openflexo.org/images/components/ta/http_ta/HTTP200x100.png">
	</menu>

    <menu name="Openflexo HTTP">
      <item name="Home" 		href="index.html" />
      <item name="Javadoc"    	href="./apidocs/index.html" />
    </menu>

		<menu ref="modules" />
		<menu ref="reports" />
	</body>

</project>
